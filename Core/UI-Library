local id = 17538292979
print("Fire-Library ID: "..id)

local uiHolder = gethui and gethui() or (not game.UserInputService.KeyboardEnabled and game.UserInputService.TouchEnabled) and game.Players.LocalPlayer.PlayerGui or table.pack(pcall(function()return game.CoreGui:GetFullName() end))[1] and game.CoreGui or game.Players.LocalPlayer.PlayerGui

local root = game:GetObjects("rbxassetid://"..id)
local lib = uiHolder:FindFirstChild("FireLib") or root[#root]:Clone()

lib.Parent = game.TextChatService
local module = lib:FindFirstChildOfClass("ModuleScript")

coroutine.wrap(function()
    task.wait(0.1)
    lib.Parent = uiHolder
end)()

local function isEmptyString(str)
    return not not str and str:gsub("\10",""):gsub("\13",""):gsub(" ",""):gsub("\9","") == "" or not str
end

local loadedSource

pcall(function()
    loadedSource = "local script = game.TextChatService[\""..lib.Name.."\"]:FindFirstChildOfClass(\"ModuleScript\")\10"..module.Source
end)

if not isEmptyString(loadedSource) then
    return loadstring(loadedSource)()
else
    return require(module)
end
